using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Rotate : MonoBehaviour
{
    public Vector3 rotationSpeed = new Vector3(0, 30, 0); // Adjust the rotation speed as needed

    void Update()
    {
        // Rotate the object around its local axis based on rotationSpeed and deltaTime
        transform.Rotate(rotationSpeed * Time.deltaTime);
    }
}

